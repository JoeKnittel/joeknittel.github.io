library(tidyverse)
data = read_csv("http://joeknittel.github.io/sample_data.csv")
trans = data %>% group_by(county) %>% summarize(tiv2011 = sum(tiv_2011), tiv2012 = sum(tiv_2012), delta = (tiv2012-tiv2011)/tiv2011) %>% arrange(desc(delta))
View(trans)
library(urbnmapr)
